{
  "query": {
    "match" : {
      "_all" : "pum"
    }
  },
  "aggregations": {
    "basisofrecord": {
      "filter": {
        "terms": {"typestatus": ["SYNTYPE"]}
      },
      "aggregations": {
        "filtered_basisofrecord": {
          "terms": {"field": "basisofrecord"}
        }
      }
    },
    "typestatus": {
      "filter": {
        "terms": {"basisofrecord": ["PRESERVED_SPECIMEN"]}
      },
      "aggregations": {
        "filtered_typestatus": {
          "terms": {"field": "typestatus"}
        }
      }
    }
  },
  "post_filter": {
    "bool": {
      "must": [
        {"terms":{"basisofrecord":["PRESERVED_SPECIMEN"]} },
        {"terms":{"typestatus": ["SYNTYPE"]} }
      ]
    }
  }
}
